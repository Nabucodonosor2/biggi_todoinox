								<td class="claro" align="center">{STOCK}</td>
								<td class="claro" align="left">{LABEL_ALERT_R}</td>
							</tr>
							<tr class="encabezado_center_informe">
								<td class="oscuro"></td>
								<td>Monto</td>
								<td class="oscuro" align="center">{SUM_TOT_FA}</td>
								<td class="oscuro" align="center">{SUM_TOT_NC}</td>
								<td class="oscuro" align="center">{SUM_TOTAL}</td>
