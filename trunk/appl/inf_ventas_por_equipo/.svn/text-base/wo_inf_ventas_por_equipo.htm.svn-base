								<td width="6%" align="right">{CANTIDAD}</td>
								<td width="8%" align="right">{PRECIO}</td>
								<td width="9%" align="right"><div style="padding-right:20px;">{TOTAL}</div></td>
							</tr>
							<!-- endBlock(wo_registro) -->
						</table>
						</div>
						<table width="1000" align="center" style="width: 950px" border=1>
