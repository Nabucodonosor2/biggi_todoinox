							<tr class="encabezado_center_informe">
								<th width="8%">Detalle</th>
								<th width="8%">{H_MES}</th>
								<th width="11%">{H_COD_PRODUCTO}</th>
								<th width="8%">{H_TIPO_DOC}</th>
								<th width="8%">{H_NRO_DOC}</th>
								<th width="9%">{H_FECHA_DOC}</th>
