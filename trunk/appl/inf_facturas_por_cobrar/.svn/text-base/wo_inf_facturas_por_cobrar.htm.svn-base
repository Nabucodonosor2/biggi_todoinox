<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"/>
<title>Sistema Grupo de Empresas Biggi</title>
<link href="{W_CSS}" rel="stylesheet" type="text/css">
<script charset="iso-8859-1" src="../../../../commonlib/trunk/script_js/general.js" type="text/javascript"></script>
{WO_JAVA_SCRIPT}
</head>
<body {WO_ONLOAD}>
<!-- DO NOT MOVE! The following AllWebMenus linking code section must always be placed right AFTER the BODY tag-->
<!-- ******** BEGIN ALLWEBMENUS CODE FOR menu ******** -->
<script type="text/javascript">var MenuLinkedBy="AllWebMenus [4]",awmMenuName="menu",awmBN="740";awmAltUrl="";</script>
<script charset="UTF-8" src="{K_ROOT_URL}menu/menu.js" type="text/javascript"></script>
<script type="text/javascript">{W_MENU}</script>
<!-- ******** END ALLWEBMENUS CODE FOR menu ******** -->
<form action="" method="post" name="output" id="output">
<input name="wo_hidden" id="wo_hidden" type="hidden" value="">
<table width="1050" border="0" align="center">
	<tr bgcolor="#FFFFFF">
		<!-- menu superior-->
		<td width="1000"  height="51" align="center" id="awmAnchor-menu" style="width:100px; height:50px;"></td>
	</tr>
	<tr bgcolor="#FFFFFF">
		<td>
			<table width="1000" border="0">
				<tr>
					<td>
						<table width="592" border="0">
							<tr>							
								<td height="100%"><div class="titulo_central_parametro">{WO_RUTA_MENU}</div></td>
								<td height="0%"><div class="tit_superior_informe"><font>{WO_NOM_FILTRO}</font></div></td>
							</tr>
						</table>
					</td> 
					<td>{WO_PRINT}</td>
					<td>{WO_EXPORT}</td>
					<td></td>					
				</tr>
			</table>
		</td>
	</tr>
	<tr bgcolor="#FFFFFF">
		<td>	  
			<table width="1000" border="0" align="center" height="550">
				<tr bgcolor="#FFFFFF" height="480">
					<td valign="top">
						<table width="1000" align="center">
							<tr class="encabezado_center_informe">
								<th width="6%">Detalle</th>
								<th width="10%">{H_NRO_FACTURA}</th>
								<th width="8%">{H_FECHA_FACTURA_STR}</th>
								<th width="9%">{H_RUT}</th>
								<th width="32%">{H_NOM_EMPRESA}</th>
								<th width="7%">{H_INI_USUARIO_VENDEDOR_A}</th>
								<th width="9%">{H_TOTAL_CON_IVA}</th>
								<th width="9%">{H_SALDO}</th>
								<th width="10%">{H_PAGO}</th>
							</tr>
						</table>
						<div id="wo_scroll" style="overflow-x:hidden; overflow-y:auto; height:450;">						
						<table width="1000" align="center">
							<!-- startBlock(wo_registro) -->
							<tr class="{WO_TR_CSS}">
								<td width="6%" align="center">{WO_DETALLE}</td>
								<td width="10%" align="center">{NRO_FACTURA}</td>
								<td width="8%" align="center">{FECHA_FACTURA_STR}</td>
								<td width="9%" align="center">{RUT}-{DIG_VERIF}</td>
								<td width="32%" align="left">{NOM_EMPRESA}</td>
								<td width="7%" align="center">{INI_USUARIO_VENDEDOR_A}</td>
								<td width="9%" align="right">{TOTAL_CON_IVA}</td>
								<td width="9%" align="right">{SALDO}</td>
								<td width="10%" align="right"><div style="padding-right:20px;">{PAGO}</div></td>
							</tr>
							<!-- endBlock(wo_registro) -->
						</table>
						</div>
					</td>
				</tr>		
				<tr bgcolor="#FFFFFF" height="28">
					<td>						
						<table width="1000" align="center" class="tabla_informe" style="width: 950px">
							<tr>
								<td width="115" align="right" class="negrita">Cantidad facturas</td>
								<td width="3">:</td>
								<td width="30" align="right"> <strong><font size="2">{SUM_CANTIDAD_FA}</font></strong></td>
								<td width="30" align="center"></td>
								<td width="80" align="right" class="negrita">Total facturas</td>
								<td width="3">:</td>
								<td width="70" align="right"> <strong><font size="2">{SUM_TOTAL_CON_IVA}</font></strong></td>
								<td width="30" align="center"></td>
								<td width="120" align="right" class="negrita">Saldo por cobrar</td>
								<td width="3">:</td>
								<td width="70" align="right"> <strong><font size="2">{SUM_SALDO}</font></strong></td>
								<td width="30" align="center"></td>
								<td width="115" align="right" class="negrita">Pagos</td>
								<td width="3">:</td>
								<td width="70" align="right"> <strong><font size="2">{SUM_PAGO}</font></strong></td>
							</tr>
						</table>
					</td>
				</tr>
				<tr bgcolor="#FFFFFF" height="28">
					<td>						
						<table width="1000" border="0" align="center">
							<tr valign="middle">
								<td width="290" >{WO_BACK}</td>
								<td align="right">{WO_FIRST}{WO_PREV}</td>
								<td align="center" class="cant_paginas" width="230">{WO_PAGES}</td>
								<td align="left">{WO_NEXT}{WO_LAST}</td>
								<td width="180"></td>
							</tr>
						</table> 
					</td>
				</tr>
			</table>
		</td>
	</tr>
</table>
</form>
</body>
</html>