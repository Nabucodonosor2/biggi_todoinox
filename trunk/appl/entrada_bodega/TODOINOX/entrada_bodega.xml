<Report>
	<Font name="Arial" size="7" style="" id="ArialSmall"/>
	<Font name="Arial" size="7" style="" id="ArialSmallBlue" red="0" green="0" blue="128"/>
	<Font name="Arial" size="17" style="B" id="ArialBoldBigB" red="0" green="0" blue="128"/>
	<Font name="Arial" size="10" style="B" id="ArialBold"/>
	<Font name="Arial" size="10" style="" id="Arial"/>
	<Font name="Arial" size="8" style="B" id="ArialBold_header"/>	
	<Font name="Arial" size="10" style="B" id="ArialBoldBlue" red="0" green="0" blue="128"/>
	<PageHeader>	
		<Cell text="" height="55%" newline="1"/>		
		<Cell text="PAG. {SectionPageNumber}/{snb}" align="R" font="ArialSmall" newline="1" />
		<Cell text="ENTRADA A BODEGA Nº {strCOD_ENTRADA}" width="100%" align="C" newline="1" font="ArialBoldBigB"/>
		<Cell text="" height="4" newline="1"/>	
	</PageHeader>
	<Content font="Arial">
		<Cell text="{ITEM}" width="5%" align="C" border="LRTB" font="Arial"/>	
		<Cell text="{COD_PRODUCTO}" width="14%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{NOM_PRODUCTO}" width="56%" align="L" border="LRTB" font="Arial"/>
		<Cell text="{PRECIO}" width="12%" align="R" border="LRTB" font="Arial"/>		
		<Cell text="{CANTIDAD}" width="12%" align="R" border="LRTB" font="Arial" newline="1"/>	
	</Content>
	<Group>
		 <Variable name="sum_total" function="SUM" value="TOTAL"/>
		<GroupHeader>				
			<Cell height="15" newline="1"/>
			<Cell text="Fecha Entrada: {FECHA_ENTRADA_BODEGA}" width="70%" align="L"  font="ArialBoldBlue"/>
			<Cell text="Bodega: {NOM_BODEGA}" height="12" width="30%" align="L"  font="ArialBoldBlue" newline="1"/>
			<Cell text="Hora Entrada: {HORA_ENTRADA_BODEGA}" width="30%" align="L"  font="ArialBoldBlue" newline="1"/>
			<Cell text="N° Factura Proveedor:" width="20%" align="L" font="ArialBoldBlue"/>
			<Cell text="{NRO_FACTURA_PROVEEDOR}" width="15%" align="L" font="Arial" newline="1"/>
			<Cell text="Referencia                  :" width="20%" align="L" font="ArialBoldBlue"/>
			<Cell text="{REFERENCIA}" width="50%" align="L" font="Arial" newline="1"/>
			<Cell text="Documento Entrada  :" width="20%" align="L" font="ArialBoldBlue"/>
			<Cell text="{TIPO_DOC}" width="15%" align="L" font="Arial" newline="1"/>
			<Cell text="N° Documento           :" width="21%" align="L" font="ArialBoldBlue"/>
			<Cell text="{COD_DOC}" width="15%" align="L" font="Arial" newline="1"/>
			<Cell text="Fecha Documento     :" width="20%" align="L" font="ArialBoldBlue"/>
			<Cell text="{}" width="15%" align="L" font="Arial" newline="1"/>
			<Cell text="" width="15%" align="L" font="Arial"/>
			<Cell text="" width="60%" align="L" font="Arial" newline="1"/>
			<Cell text="__________________________________________________________________________________________________" width="99%" align="L" font="Arial"/>
			<Cell text="" height="4" newline="1"/>
			<Cell text="" height="4" newline="1"/>
			<Cell text="" height="5" newline="1"/>		
			<Cell text="Item" width="5%" align="C" border="LRTB" font="ArialBoldBlue"/>		
			<Cell text="Modelo" width="14%" align="C" border="LRTB" font="ArialBoldBlue"/>
			<Cell text="Equipo" width="56%" align="C" border="LRTB" font="ArialBoldBlue"/>
			<Cell text="Precio" width="12%" align="C" border="LRTB" font="ArialBoldBlue"/>
			<Cell text="Cantidad" width="12%" align="C" border="LRTB" font="ArialBoldBlue" newline="1"/>
		</GroupHeader>
		<GroupFooter>
		    <Cell text="   TOTAL" width="77%" align="R" font="Arial"/>
			<Cell text="{TOTAL_ENTRADA}" width="11%" align="R" font="Arial" newline="1"/>
			<Cell text="" width="15%" align="L" font="Arial"/>
			<Cell text="" width="60%" align="L" font="Arial" newline="1"/>
			<Cell text="OBSERVACIÓN:" width="15%" align="L" font="ArialBoldBlue"/>
			<Cell text="{OBS}" width="60%" align="L" font="Arial" newline="1"/>
			<Cell height="30" newline="1"/>
			<Cell text="" width="10%" align="L" font="Arial"/>
			<Cell text="" width="15%" align="L" font="Arial"/>
			<Cell text="" width="60%" align="L" font="Arial" newline="1"/>
			<Cell text="Recepcionado Por" width="20%" border="T" align="C" font="Arial"/>
			<Cell text="" width="40%" align="L" font="Arial"/>
			<Cell text="Entregado Por" width="20%" border="T" align="C" font="Arial"/>
			<Cell height="30" newline="1"/>
			<Cell text="" width="75%" align="L" font="Arial"/>
		</GroupFooter> 
	</Group>
</Report>