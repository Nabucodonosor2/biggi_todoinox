<script type="text/javascript">
function get_returnVal() {
	var cod_empresa			= document.getElementById('COD_EMPRESA_0').value;
	var cod_cta_corriente	= document.getElementById('COD_CUENTA_CORRIENTE_0').value;
	
	if(cod_empresa != '' && cod_cta_corriente != ''){
		return cod_empresa + '|' + cod_cta_corriente;
	}else{
		alert('Faltan ingresar datos.')
		return false;	
	}
}
</script>
</head>
<body onload="document.getElementById('COD_EMPRESA_0').focus();">
<div class="titulo_tabla_wm">&nbsp;&nbsp;Seleccione una Empresa y Cuenta Corriente</div>
<form id="form1" name="form1" method="post" action="">
	<table width="435" border="0" align="left">  
  		<tr>	
  			<td>
  				<table width="580" border="0" align="center">  					
					<tr class="encabezado_right">
						<td width="20%">Codigo</td>
    					<td width="80%" class="claro" align="left">{COD_EMPRESA}</td>
  					</tr>
  					<tr class="encabezado_right">
						<td>Rut</td>
    					<td class="oscuro" align="left">{RUT}-{DIG_VERIF}</td>
  					</tr>
  					<tr class="encabezado_right">
						<td>Alias</td>
    					<td class="claro" align="left">{ALIAS}</td>
  					</tr>
  					<tr class="encabezado_right">
						<td>Razon Social</td>
    					<td class="oscuro" align="left">{NOM_EMPRESA}</td>
  					</tr>
  					<tr class="encabezado_right">
						<td>Cuenta Corriente</td>
    					<td class="oscuro" align="left">{COD_CUENTA_CORRIENTE}</td>
  					</tr>
				</table>
  			</td>
  		</tr>
		<tr align="left"> 
			<td align="right">
				<input name="OK" type="button" value="OK"  onclick="returnValue=get_returnVal(); if(returnValue != false) window.close();"/>
				<input name="Cancelar" type="button" value="Cancelar" onclick="returnValue=null; window.close();"/>
			</td>
  		</tr>
	</table>
</form>
</body>
