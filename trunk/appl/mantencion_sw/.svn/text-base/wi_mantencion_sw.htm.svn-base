<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<title>Sistema Grupo de Empresas Biggi</title>
<link href="{W_CSS}" rel="stylesheet" type="text/css">
<script src="../../../../commonlib/trunk/script_js/SpryTabbedPanels.js" type="text/javascript"></script>
<link href="../../../../commonlib/trunk/css/SpryTabbedPanels.css" rel="stylesheet" type="text/css"/>
<script charset="iso-8859-1" src="../../../../commonlib/trunk/script_js/edit_filter.js" type="text/javascript"></script>
<script charset="iso-8859-1" src="../../../../commonlib/trunk/script_js/general.js" type="text/javascript"></script>
<script charset="iso-8859-1" src="mantencion_sw.js" type="text/javascript"></script>
<script charset="UTF-8" src="" type="text/javascript"></script>
</head>
<body {WI_ONLOAD}>
{WI_JAVA_SCRIPT}
<!-- DO NOT MOVE! The following AllWebMenus linking code section must always be placed right AFTER the BODY tag-->
<!-- ******** BEGIN ALLWEBMENUS CODE FOR menu ******** -->
<script type="text/javascript">var MenuLinkedBy="AllWebMenus [4]",awmMenuName="menu",awmBN="740";awmAltUrl="";</script>
<script charset="UTF-8" src="{K_ROOT_URL}menu/menu.js" type="text/javascript"></script>
<script type="text/javascript">{W_MENU}</script>
<!-- ******** END ALLWEBMENUS CODE FOR menu ******** -->
<form action="" method="post" name="input" id="input">
<input name="wi_hidden" id="wi_hidden" type="hidden" value="">
<table width="1050" border="0" align="center">
	<tr bgcolor="#FFFFFF">
		<!-- menu superior-->
		<td width="1000" height="51" align="center" id="awmAnchor-menu" style="width: 100px; height: 50px;"></td>
	</tr>
	<tr bgcolor="#FFFFFF">
		<td>
		<table width="1000" border="0">
			<tr>
				<td>
				<table width="520" border="0">
					<tr>
						<td height="100%">
						<div class="titulo_central">{WI_RUTA_MENU}</div>
						</td>
					</tr>
					<tr>
						<td height="0%">
						<div class="tit_superior"><font color="#999999">{WI_FECHA_ACTUAL}</font></div>
						</td>
					</tr>
					<tr>
						<td height="0%">
						<div class="tit_superior"><font color="#999999">{WI_FECHA_MODIF}</font></div>
						</td>
					</tr>
				</table>
				</td>
				<td width="68"></td>
				<td width="68">{WI_PRINT}</td>
				<td width="68">{WI_NO_SAVE}</td>
				<td width="68">{WI_SAVE}</td>
				<td width="68">{WI_MODIFY}</td>
			</tr>
		</table>
		</td>
	</tr>
	<tr bgcolor="#FFFFFF">
		<td>
		<table width="1000" border="0" align="center">
			<tr>
				<td height="95">
				<table width="1015" border="0" align="center">
					<tr valign="top">
						<td>
						<div id="TabbedPanels1" class="TabbedPanels">
						<ul class="TabbedPanelsTabGroup">
							<li class="TabbedPanelsTab" tabindex="0" style="display:{TAB_100505}">Mantencion</li>
						</ul>
						<div class="TabbedPanelsContentGroup">
							<!--INI MANTENCION-->
							<div class="TabbedPanelsContent" style="height: 492px;">
							<table width="1000" rules="none" border="1" align="center"
								class="claro">
								<tr class="titulo_tabla">
									<td colspan="6">DATOS GENERALES</td>
								</tr>
								<tr class="encabezado_right">
									<td width="166">Código:</td>
									<td width="166" class="claro" align="left">{COD_MANTENCION_SW}</td>
									<td width="166">Fecha:</td>
									<td width="166" class="claro" align="left">{FECHA_MANTENCION_SW}</td>
									<td width="166">Usuario:</td>
									<td width="166" class="claro" align="left">{NOM_USUARIO}</td>
								</tr>
								<tr class="encabezado_right">
									<td>Módulo</td>
									<td class="oscuro" align="left">{COD_ITEM_MENU}</td>
									<td>Solicitado Por:</td>
									<td class="oscuro" align="left">{COD_USUARIO_SOLICITA}</td>
									<td>Estado:</td>
									<td class="oscuro" align="left">{NOM_ESTADO_SOLUCION_SW}</td>
								</tr>
								<tr class="encabezado_right">
									<td>Referencia:</td>
									<td colspan="3" class="claro" align="left">{Referencia}</td>
									<td>Autoriza:</td>
									<td class="claro" align="left">{AUTORIZA} {COD_USUARIO_AUTORIZA}</td>
									<!-- 
									<td>Garantía:</td>
									<td colspan="1" class="oscuro" align="left">{ES_GARANTIA}</td>
									 -->
								</tr>
								<tr class="encabezado_right">
									<td>Descripción:</td>
									<td colspan="6" class="oscuro" align="left">{descripcion}</td>
								</tr>
							</table>
							<br/>
							
							<table width="1000" border="1" rules="none" align="center">
								<tr class="titulo_tabla">
									<td colspan="6">DATOS SOLUCIÓN</td>
								</tr>
							</table>
							<div style="overflow-x: hidden; overflow-y: auto; height: 260;">
								<table width="1000" rules="none" border="1" align="center" class="claro" id="SOLUCION">
									<!-- startBlock(SOLUCION) -->
									<tr class="encabezado_right">
										<td width="166">Iteración:</td>
										<td width="166" class="claro" align="left">{NRO_ITERACION}</td>
										<td width="150">Desarrollador:</td>
										<td width="150" class="claro" align="left">{COD_DESARROLLADOR_SW}</td>
										<td width="150">Estado:</td>
										<td class="claro" align="left">{COD_ESTADO_SOLUCION_SW}</td>
									</tr>
									<tr class="encabezado_right">
										<td>Inicio:</td>
										<td class="oscuro" align="left">{FECHA_INICIO}</td>
										<td>Termino:</td>
										<td class="oscuro" align="left">{FECHA_TERMINO}</td>
										<td>Tiempo:</td>
										<td class="oscuro" align="left" style="display:{DISPLAY_MINUTOS}">{MINUTOS} (minutos)</td>
									</tr>
									<tr class="encabezado_right" style="display:{DISPLAY_INTERNO}">
										<td>Solución Interna:</td>
										<td colspan="6" class="claro" align="left">{SOLUCION_INTERNA}</td>
									</tr>
									<tr class="encabezado_right">
										<td>Solución:</td>
										<td colspan="6" class="claro" align="left">{SOLUCION_CLIENTE}</td>
									</tr>
									<tr class="encabezado_right" style="display:{DISPLAY_RESPUESTA_CLIENTE}" id="TR_RESPUESTA_CLIENTE">
										<td>Respuesta Cliente:</td>
										<td colspan="6" class="claro" align="left">{RESPUESTA_CLIENTE}</td>
									</tr>
									<tr class="encabezado_right" style="height:10">
										<td bgcolor="507FFF" colspan="6"></td>
									</tr>
									<!-- endBlock(SOLUCION) -->
								</table>
							</div>
						</div>
						<!--FIN MANTENCION -->
						</div>
						</div>
						<!--FIN TabbedPanelsContentGroup-->
						</td>
					</tr>
				</table>
				<table width="1000" border="0" align="center">
					<tr valign="middle">
						<td width="290">{WI_BACK}</td>
						<td align="right">{WI_FIRST}{WI_PREV}</td>
						<td align="left">{WI_NEXT}{WI_LAST}</td>
						<td width="290"></td>
					</tr>
				</table>
				</td>
			</tr>
		</table>
		</td>
	</tr>
</table>
</form>
<script type="text/javascript">
<!--
var TabbedPanels1 = new Spry.Widget.TabbedPanels("TabbedPanels1");
//-->
</script>
</body>
</html>