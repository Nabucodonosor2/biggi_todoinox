	</tr>
	<tr>
		<td>
			<table width="610" rules="none" border="1" align="left" class="claro">	
			<!-- startBlock(DETALLE_CAMBIO) -->
				<tr class="{DW_TR_CSS}" id="{DW_TR_ID}">
					<td width="160" align="left">{NOM_USUARIO}</td>
					<td width="150" align="center">{FECHA_CAMBIO}</td>
					<td width="160" align="center">{VALOR_ANTIGUO}</td>
					<td width="160" align="center">{VALOR_NUEVO}</td>   
				</tr>
			<!-- endBlock(DETALLE_CAMBIO) -->	
			</table>		
		</td>
	</tr>
