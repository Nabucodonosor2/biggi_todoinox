		<Cell text="{TOT_MONTO_IVA}" width="15%" align="R" border="LR" font="ArialItem" format="N0,." newline2="1" />

		<Cell text="" width="15%"/>
		<Cell text="Total con IVA:" width="25%" align="R" border="LRB" font="ArialItem"/>
		<Cell text="{FA_TOTAL_CON_IVA}" width="15%" align="R" border="LRB" font="ArialItem" format="N0,." />
		<Cell text="{NC_TOTAL_CON_IVA}" width="15%" align="R" border="LRB" font="ArialItem" format="N0,." />
		<Cell text="{TOT_TOTAL_CON_IVA}" width="15%" align="R" border="LRB" font="ArialItem" format="N0,." newline2="1"/>
