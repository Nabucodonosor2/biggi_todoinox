	<Font name="Arial" size="9" style="BU" id="ArialBoldUItem"/>
	
	<PageHeader>	
		<Cell text="" height="100" newline="1"/>		

		<Cell text="" width="30%"/>		
		<Cell text="FACTURAS POR MES" width="40%" align="C" font="ArialBoldBigB" newline="1"/>

		<Cell text="" width="30%"/>		
		<Cell text="AÃ±o: {strANO}" width="40%" align="C" font="ArialBoldBlue"  newline="1"/>
		<Cell text="" width="30%"/>		
		<Cell text="Desde: {strMES_DESDE}; Hasta: {strMES_HASTA}" width="40%" align="C" font="ArialBoldBlue" newline="1"/>
	</PageHeader>
	<Content font="Arial">
		<Cell text="" height="50" newline2="1"/>		

		<Cell text="" width="15%"/>
		<Cell text="" width="25%" align="R" border="LRT" font="ArialItem"/>
		<Cell text="Facturas" width="15%" align="C" border="LRT" font="ArialItem"/>
		<Cell text="NC" width="15%" align="C" border="LRT" font="ArialItem"/>
		<Cell text="Total" width="15%" align="C" border="LRT" font="ArialItem" newline2="1" />

		<Cell text="" width="15%"/>
		<Cell text="Cantidad:" width="25%" align="R" border="LRT" font="ArialItem"/>
		<Cell text="{FA_CANT}" width="15%" align="R" border="LRT" font="ArialItem"/>
		<Cell text="{NC_CANT}" width="15%" align="R" border="LRT" font="ArialItem"/>
		<Cell text="{TOT_CANT}" width="15%" align="R" border="LRT" font="ArialItem" newline2="1" />
		
		<Cell text="" width="15%"/>
		<Cell text="Total Neto:" width="25%" align="R" border="LR" font="ArialItem"/>
		<Cell text="{FA_TOTAL_NETO}" width="15%" align="R" border="LR" font="ArialItem" format="N0,."/>
		<Cell text="{NC_TOTAL_NETO}" width="15%" align="R" border="LR" font="ArialItem" format="N0,."/>
		<Cell text="{TOT_TOTAL_NETO}" width="15%" align="R" border="LR" font="ArialItem" format="N0,." newline2="1" />
		
		<Cell text="" width="15%"/>
