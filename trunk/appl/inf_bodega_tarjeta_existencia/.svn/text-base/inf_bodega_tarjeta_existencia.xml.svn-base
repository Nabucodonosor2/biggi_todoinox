<Report>
	<Font name="Arial" size="7" style="" id="ArialSmall"/>
	<Font name="Arial" size="17" style="B" id="ArialBoldBigB" red="0" green="0" blue="128"/>
	<Font name="Arial" size="8" style="B" id="ArialBold"/>
	<Font name="Arial" size="8" style="B" id="ArialBold_header"/>	
	<Font name="Arial" size="10" style="" id="ArialSmallBlue" red="0" green="0" blue="128"/>
	<Font name="Arial" size="9" style="" id="Arial"/>
	<PageHeader>	
		<Cell text="" height="55%" newline="1"/>		
		<Cell text="PAG. {SectionPageNumber}/{snb}" align="R" font="ArialSmall" newline="1" />
		<Cell text="TARJETA EXISTENCIA 'BODEGA STOCK BIGGI'" width="100%"  align="C" newline="1" font="ArialBoldBigB"/>
		<Cell text="{str_filtro}" width="100%" align="C" newline="1" font="ArialSmallBlue"/>
		<Cell text="" height="4" newline="1"/>
		<Cell text="" height="4" newline="1"/>
		<Cell text="" height="4" newline="1"/>	
	</PageHeader>
	<Content font="Arial">
		<Cell text="{FECHA_STR}" width="11%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{REFERENCIA}" nowrap="yes" width="40%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{TIPO_DOC}" width="20%" align="C" border="LRTB" font="Arial"/>
		<Cell text="{COD_DOC}" width="8%" align="R" border="LRTB" font="Arial"/>	
		<Cell text="{CANTIDAD_ENTRADA}" width="7%" align="R" border="LRTB" format="N1,." font="Arial"/>
		<Cell text="{CANTIDAD_SALIDA}" width="7%" align="R" border="LRTB" format="N1,." font="Arial" />
		<Cell text="{CANTIDAD_STOCK}" width="7%" align="R" border="LRTB" format="N1,." font="Arial" newline="1"/>
	</Content>
	<Group>
		<GroupHeader>
			<Cell text="FECHA" width="11%" align="C" border="LRTB" font="ArialSmallBlue"/>		
			<Cell text="REFERENCIA" width="40%" align="C" border="LRTB" font="ArialSmallBlue"/>		
			<Cell text="TIPO DOC" width="20%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="NÂº DOC" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="ENT." width="7%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="SAL." width="7%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="STOCK" width="7%" align="C" border="LRTB" font="ArialSmallBlue" newline="1"/>
		</GroupHeader>
	</Group>
</Report>