<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"/>
<title>Sistema Grupo de Empresas Biggi</title>
<link href="{W_CSS}" rel="stylesheet" type="text/css">
<script charset="iso-8859-1" src="../../../../commonlib/trunk/script_js/general.js" type="text/javascript"></script>
{WO_JAVA_SCRIPT}
</head>
<body {WO_ONLOAD}>
<!-- DO NOT MOVE! The following AllWebMenus linking code section must always be placed right AFTER the BODY tag-->
<!-- ******** BEGIN ALLWEBMENUS CODE FOR menu ******** -->
<script type="text/javascript">var MenuLinkedBy="AllWebMenus [4]",awmMenuName="menu",awmBN="740";awmAltUrl="";</script>
<script charset="UTF-8" src="{K_ROOT_URL}menu/menu.js" type="text/javascript"></script>
<script type="text/javascript">{W_MENU}</script>
<!-- ******** END ALLWEBMENUS CODE FOR menu ******** -->
<form action="" method="post" name="output" id="output">
<input name="wo_hidden" id="wo_hidden" type="hidden" value="">
<table width="1050" border="0" align="center">
	<tr bgcolor="#FFFFFF">
		<!-- menu superior-->
		<td width="1000"  height="51" align="center" id="awmAnchor-menu" style="width:100px; height:50px;"></td>
	</tr>
	<tr bgcolor="#FFFFFF">
		<td>
			<table width="1000" border="0">
				<tr>
					<td>
						<table width="592" border="0">
							<tr>							
								<td height="100%"><div class="titulo_central_parametro">{WO_RUTA_MENU}</div></td>
								<td height="0%"><div class="tit_superior_informe"><font>{WO_NOM_FILTRO}</font></div></td>
							</tr>
						</table>
					</td>
					<td>{WO_PRINT}</td>
					<td></td>			
				</tr>
			</table>
		</td>
	</tr>
	<tr bgcolor="#FFFFFF">
		<td>	  
			<table width="900" border="0" align="center" height="600">
				<tr bgcolor="#FFFFFF" height="480">
					<td valign="top">
						<table width="1000" align="center">
							<tr class="encabezado_center_informe">
								<th width="5%">Detalle</th>
								<th width="8%">{H_NRO_FACTURA}</th>
								<th width="8%">{H_FECHA}</th>
								<th width="39%">{H_NOM_EMPRESA}</th>
								<th width="10%">{H_TOTAL_CON_IVA}</th>
								<th width="10%">{H_NOM_FORMA_PAGO}</th>
								<th width="10%">{H_MONTO_PAGOS}</th>
								<th width="10%">{H_SALDO}</th>
							</tr>
						</table>
						<div id="wo_scroll" style="overflow-x:hidden; overflow-y:auto; height:270;">						
						<table width="1000" align="center">
							<!-- startBlock(wo_registro) -->
							<tr class="{WO_TR_CSS}">
								<td width="5%" align="center">{WO_DETALLE}</td>
								<td width="8%" align="center">{NRO_FACTURA}</td>
								<td width="8%" align="center">{FECHA}</td>
								<td width="39%" align="left">{NOM_EMPRESA}</td>
								<td width="10%" align="right">{TOTAL_CON_IVA}</td>
								<td width="10%" align="center">{NOM_FORMA_PAGO}</td>
								<td width="10%" align="right">{MONTO_PAGOS}</td>
								<td width="10%" align="right"><div style="padding-right:20px;">{SALDO}</div></td>
							</tr>
							<!-- endBlock(wo_registro) -->
						</table>
						</div>
						<table width="1000" align="center">
							<tr class="encabezado_center_informe">
								<td width="5%"></td>
								<td width="8%"></td>
								<td width="8%"></td>
								<td width="39%"></td>
								<td width="10%" align="right">{SUM_TOTAL_CON_IVA}</td>
								<td width="10%"></td>
								<td width="10%" align="right">{SUM_MONTO_PAGOS}</td>
								<td width="10%" align="right">{SUM_SALDO}</td>
							</tr>
						</table>
						<p>
						<table width="1000" rules="none" border="1" align="center" class="claro">
							<tr class="titulo_tabla">
								<td colspan="11">Documentos</td>
							</tr>
							<tr class="encabezado_center">
								<th width="10%">Tipo Doc</th>
								<th width="10%">cant</th>
								<th width="10%">Monto</th>
							</tr>
						</table>
						<div style="overflow-x:hidden; overflow-y:auto; height:120; margin-top:-1em;">
						<table width="1000" rules="none" border="1" align="center" class="claro" id="DW_DOCS">
							<!-- startBlock(DW_DOCS) -->
							<tr class="{DW_TR_CSS}" id="{DW_TR_ID}">
								<td width="10%" align="left">{NOM_TIPO_DOC_PAGO}</td>
								<td width="10%" align="center">{CANT}</td>
								<td width="10%" align="right">{MONTO}</td>
							</tr>
							<!-- endBlock(DW_DOCS) -->
						</table>
						</div>
						<table width="1000" align="center">
							<tr class="encabezado_center_informe">
								<td width="10%"></td>
								<td width="10%"></td>
								<td width="10%" align="right">{SUM_MONTO}</td>
							</tr>
						</table>


					</td>
				</tr>		
				<tr bgcolor="#FFFFFF" height="28">
					<td>						
						<table width="900" border="0" align="center">
							<tr valign="middle">
								<td width="290" >{WO_BACK}</td>
								<td align="right">{WO_FIRST}{WO_PREV}</td>
								<td align="center" class="cant_paginas" width="230">{WO_PAGES}</td>
								<td align="left">{WO_NEXT}{WO_LAST}</td>
								<td width="180"></td>
							</tr>
						</table> 
					</td>
				</tr>
			</table>
		</td>
	</tr>
</table>
</form>
</body>
</html>